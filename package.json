{
  "name": "jsonrpc-http-server",
  "description": "Server running jsonrpc-http for a given API",
  "version": "0.2.0",
  "main": "./lib/server.js",
  "dependencies": {
    "rester-tools": "0.5.0",
    "introspect": "0.0.2",
    "express": "3.0.5"
  },
  "devDependencies": {
    "underscore": "1.4.3",
    "should": "1.2.1",
    "jscov": "0.1.1",
    "mocha-term-cov-reporter": "0.1.0",
    "bufferstream": "0.5.1",
    "coffee-script": "1.4.0",
    "mocha": "1.7.4",
    "request": "2.12.0"
  },
  "engines": {
    "node": ">= 0.8"
  },
  "scripts": {
    "pretest": "coffee -co lib src",
    "prepublish": "coffee -co lib src",
    "start": "bin/locke",
    "coverage": "jscov src .cov && SRC_DIR=.cov mocha --compilers coffee:coffee-script --reporter mocha-term-cov-reporter tests/*.coffee",
    "test": "mocha --compilers coffee:coffee-script tests/*.coffee"
  }
}
